# Build dependencies 
-buildpath: \
	osgi.annotation;version='7.0',\
	osgi.core;version='6.0',\
	junit-jupiter-api;version='5.7.0',\
	osgi.cmpn,\
	assertj-core,\
	org.osgi.test.junit5,\
	org.gecko.github.template.example.api,\
	org.osgi.test.common,\
	org.mockito.mockito-core,\
	org.osgi.test.assertj.framework,\
	junit-platform-commons

Bundle-Version: 0.0.0.${tstamp}

-nojunitosgi: true
# This would execute the bndrun while Gradle build
-include:  test.bndrun